{
    "name": "xaosia/commandutils",
    "description": "Useful commands for laravel such as restoring the app:name command",
    "type": "library",
    "license": "MIT",
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "XAOSIA\\CommandUtils\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XAOSIA\\CommandUtils\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
